{
  "name": "@roundtable/cli",
  "version": "0.1.0",
  "description": "Roundtable CLI - Multi-model AI deliberation system prototype",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "roundtable": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "esbuild src/cli.ts --bundle --platform=node --outfile=dist/cli.js --format=esm --packages=external",
    "start": "node dist/cli.js",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "ai",
    "deliberation",
    "cli",
    "specification",
    "debate"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@google/generative-ai": "^0.24.1",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "openai": "^6.6.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "esbuild": "^0.19.8",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
